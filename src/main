  wclass = weapons[i][4];
  level = weapons[i][5];
  comp = weapons[i][6];
  bool win = false;
  getline(cin, guess);
  while (!win) {
    int weap = 0;
    while (weap < 79) {
      string search = weapons[weap][0];
      string csearch = weapons[weap][6];
      if (search == guess || csearch == guess) {
        gweap = weapons[weap][0];
        gsub = weapons[weap][1];
        gspec = weapons[weap][2];
        gpfs = weapons[weap][3];
        gwc = weapons[weap][4];
        glev = weapons[weap][5];
