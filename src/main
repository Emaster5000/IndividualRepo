  sub = weapons[i][1];
  special = weapons[i][2];
  pfs = weapons[i][3];
  wclass = weapons[i][4];
  level = weapons[i][5];
  bool win = false;
  getline(cin, guess);
  while (!win) {
    int weap = 0;
    while (weap < 68) {
      string search = weapons[weap][0];
      if (search == guess) {
        gweap = weapons[weap][0];
        gsub = weapons[weap][1];
        gspec = weapons[weap][2];
        gpfs = weapons[weap][3];
        gwc = weapons[weap][4];
        glev = weapons[weap][5];
